{
  "expo": {
    "name": "Sigla-Track",
    "slug": "sigla-track",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "siglatrack",
    "ios": {
      "bundleIdentifier": "com.malupait.siglatrack"
    },
    "android": {
      "package": "com.malupait.siglatrack",
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-calendar"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f9ba817c-8fdd-4d0b-91c5-c6c76fa80372"
      }
    }
  }
}
